{
  "name": "lazy-json-graphql",
  "version": "3.0.2",
  "description": "Get a full fake GraphQL API with zero coding in less than 30 seconds.",
  "keywords": [
    "graphql",
    "json",
    "schema",
    "type",
    "typedef",
    "typedefs",
    "query",
    "mutation",
    "resolver",
    "graphql-lazy"
  ],
  "homepage": "https://github.com/bluelovers/ws-graphql/tree/master/packages/json-graphql-server#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-graphql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-graphql.git"
  },
  "license": "MIT",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "sort-package-json": "npx yarn-tool sort",
    "test": "jest",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@graphql-lazy/type-date": "^1.0.2",
    "graphql-tools": "^4.0.7",
    "graphql-type-json": "^0.3.1",
    "inflection2": "^2.0.1",
    "ts-type": "^1.2.13"
  },
  "devDependencies": {
    "@types/graphql-type-json": "^0.3.2",
    "@types/inflection": "^1.5.28",
    "@types/jest": "~25.1.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "jest": "~25.1.0",
    "ts-jest": "^25.2.1"
  },
  "peerDependencies": {
    "graphql": "^14.6.0"
  },
  "authors": [
    "Fran√ßois Zaninotto",
    "Gildas Garcia"
  ]
}

{
  "name": "json-graphql-server",
  "version": "2.1.3",
  "description": "![travis (.org)](https://img.shields.io/travis/marmelab/json-graphql-server.svg) ![github top language](https://img.shields.io/github/languages/top/marmelab/json-graphql-server.svg) ![David. dependencies](https://david-dm.org/marmelab/json-graphql-server.svg) ![npm](https://img.shields.io/npm/v/json-graphql-server.svg) ![github contributors](https://img.shields.io/github/contributors/marmelab/json-graphql-server.svg) ![license](https://img.shields.io/github/license/marmelab/json-graphql-server.svg) ![prs welcome](https://img.shields.io/badge/prs-welcome-brightgreen.svg)",
  "homepage": "https://github.com/marmelab/json-graphql-server#readme",
  "bugs": {
    "url": "https://github.com/marmelab/json-graphql-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marmelab/json-graphql-server.git"
  },
  "license": "MIT",
  "author": "bluelovers",
  "main": "lib/json-graphql-server.node.min.js",
  "browser": "lib/json-graphql-server.client.min.js",
  "bin": {
    "json-graphql-server": "bin/json-graphql-server.js"
  },
  "directories": {
    "example": "example"
  },
  "files": [
    "*.md",
    "lib",
    "lib",
    "bin"
  ],
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "format": "make format",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "precommit": "lint-staged",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "prepublish": "make build",
    "server": "make run",
    "sort-package-json": "npx yarn-tool sort",
    "test": "jest",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json",
    "watch-test": "make watch-test"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-client": "~2.6.8",
    "apollo-test-utils": "~0.3.2",
    "cors": "^2.8.5",
    "express": "~4.17.1",
    "express-graphql": "~0.9.0",
    "graphql": "~14.6.0",
    "graphql-tag": "~2.10.3",
    "graphql-tools": "~4.0.7",
    "graphql-type-json": "~0.3.1",
    "inflection": "~1.12.0",
    "lodash.merge": "~4.6.2",
    "reify": "~0.20.12",
    "xhr-mock": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.8.4",
    "@babel/core": "~7.8.7",
    "@babel/plugin-external-helpers": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.8.3",
    "@babel/preset-env": "~7.8.7",
    "@types/babel__core": "^7.1.6",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.8",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/inflection": "^1.5.28",
    "@types/jest": "~25.1.4",
    "@types/lodash.merge": "^4.6.6",
    "@types/prettier": "^1.19.0",
    "@types/supertest": "^2.0.8",
    "@types/webpack": "^4.41.7",
    "babel-eslint": "10.1.0",
    "babel-jest": "~25.1.0",
    "babel-loader": "~8.0.6",
    "babel-plugin-add-module-exports": "~1.0.2",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-import": "~2.20.1",
    "eslint-plugin-jest": "~23.8.2",
    "eslint-plugin-prettier": "~3.1.2",
    "husky": "~4.2.3",
    "jest": "~25.1.0",
    "lint-staged": "~10.0.8",
    "prettier": "~1.19.1",
    "supertest": "~4.0.2",
    "ts-jest": "^25.2.1",
    "webpack": "~4.42.0",
    "webpack-cli": "~3.3.11"
  },
  "authors": [
    "Fran√ßois Zaninotto",
    "Gildas Garcia"
  ]
}
